@using Innostar.UI.Areas.Administration.ViewModels
@model Application.Core.Data.IPageableList<ErrorViewModel>

@{
	ViewBag.Title = "Лог ошибок";
	Layout = "../Shared/_MainLayout.cshtml";
}

<table class="table table-striped table-bordered table-hover">
	<tr>
		<th>Время ошибки
		</th>
		<th>GUID ошибки</th>
	</tr>

	@foreach (var item in Model.Items)
 {
		<tr>
			<td class="title">
				@Html.DisplayFor(modelItem => item.Time)
			</td>
			<td>
				@Html.ActionLink(item.Title, "Error", new { id = item.Id }) 
			</td>
		</tr>
 }

</table>

@Html.Partial("_Paging", Model)
